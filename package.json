{
  "name": "refunds",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "concurrently": "^3.5.1",
    "react": "^16.2.0",
    "react-content-loader": "^3.1.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-scripts": "1.1.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --verbose --coverage",
    "eject": "react-scripts eject",
    "mocks": "node ./node_modules/json-server/bin/index.js --watch ./mocks/db.json --port 3001 --delay 4000 --middlewares ./mocks/middlewares/login-middleware.js",
    "dev": "concurrently \"npm start\" \"npm run mocks\""
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "setupFiles": [
      "./src/__testmocks__/localStorage.mock.js"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils",
      "./node_modules/fbjs"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-jest": "^22.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "enzyme": "^3.3.0",
    "jest-cli": "^22.1.4",
    "json-server": "^0.12.1",
    "mocha": "^5.0.0",
    "react-addons-test-utils": "^15.6.2"
  }
}
